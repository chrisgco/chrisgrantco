{.section pagination}
<div class="row">
  <div class="pagination">

    {.if prevPage}
    <div class="navigate left">
      <h6><a href="{prevPageUrl}"><i class="fa fa-arrow-left"></i> Next</a></h6>
    </div>
    {.end} {.if nextPage}
    <div class="navigate right">
      <h6><a href="{nextPageUrl}">Previous <i class="fa fa-arrow-right"></i></a></h6>
    </div>
    {.end}

  </div>
</div>
{.end}
