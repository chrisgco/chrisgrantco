<!-- wrapper -->
<div class="wrapper fixed-page">

  <!-- header -->
  <header class="header">

    {@|apply header.block}

  </header>
  <!-- header -->

  <!-- section -->
  <section class="works">
    {.section collection}
    {@|apply cover.block}
    {.end}

    <div class="offset">

      <!-- container -->
      {@|apply categories.block}
      <!-- container -->

      <!-- container -->
      <div class="row container">
        {.repeated section items}
        {.equal? @index 1}
        {@|apply work.block}
        {.end}

        {.equal? @index 2}
        {@|apply work.block}
        {.end}

        {.equal? @index 3}
        {@|apply work.block}
        {.end}

        {.end}

      </div>
      <!-- container -->

      <!-- container -->
      <div class="row container">
        {.repeated section items}
        {.equal? @index 4}
        {@|apply work.block}
        {.end}

        {.equal? @index 5}
        {@|apply work.block}
        {.end}

        {.equal? @index 6}
        {@|apply work.block}
        {.end}

        {.end}
      </div>
      <!-- container -->

      <!-- container -->
      <div class="row container">
        {.repeated section items}
        {.equal? @index 7}
        {@|apply work.block}
        {.end}

        {.equal? @index 8}
        {@|apply work.block}
        {.end}

        {.equal? @index 9}
        {@|apply work.block}
        {.end}

        {.end}
      </div>
      <!-- container -->

      <!-- container -->
      <div class="row container">
        {.repeated section items}
        {.equal? @index 10}
        {@|apply work.block}
        {.end}

        {.equal? @index 11}
        {@|apply work.block}
        {.end}

        {.equal? @index 12}
        {@|apply work.block}
        {.end}

        {.end}
      </div>
      <!-- container -->


      <!-- container -->
      <div class="row container">
        {.repeated section items}
        {.equal? @index 13}
        {@|apply work.block}
        {.end}

        {.equal? @index 14}
        {@|apply work.block}
        {.end}

        {.equal? @index 15}
        {@|apply work.block}
        {.end}

        {.end}
      </div>
      <!-- container -->

      <!-- container -->
      <div class="row container">
        {.repeated section items}
        {.equal? @index 16}
        {@|apply work.block}
        {.end}

        {.equal? @index 17}
        {@|apply work.block}
        {.end}

        {.equal? @index 18}
        {@|apply work.block}
        {.end}

        {.end}
      </div>
      <!-- container -->

      <!-- container -->
      <div class="row container">
        {.repeated section items}
        {.equal? @index 19}
        {@|apply work.block}
        {.end}

        {.equal? @index 20}
        {@|apply work.block}
        {.end}

        {.end}
      </div>
      <!-- container -->

    </div>

  </section>
  <!-- section -->

  <!--pagination-->
  {@|apply pagination.block}
  <!--pagination-->


  <!-- footer -->
  {@|apply footer.block}
  <!-- footer -->


</div>
<!-- wrapper -->
